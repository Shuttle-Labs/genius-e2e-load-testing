{
  "name": "my-e2e-playwright-ts-po",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js",

    "pw:install": "playwright install --with-deps",
    "pw:test": "playwright test",
    "pw:test:ui": "playwright test --ui",
    "pw:syn:cache": "synpress test/wallet-setup",

    "k6:build": "tsc -p k6/tsconfig.k6.json",
    "k6:run": "npm run k6:build && k6 run dist-k6/main.test.js"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "@solana/web3.js": "^1.98.4",
    "@synthetixio/synpress": "4.1.1",
    "@types/k6": "^1.3.1",
    "@types/node": "^20.19.24",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ed25519-hd-key": "^1.3.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.6.2",
    "k6": "^0.0.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "csv-parser": "^3.2.0",
    "ethers": "^6.0.0"
  }
}
